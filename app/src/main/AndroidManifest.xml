<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fefeyo.kyotoibw">

    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.FLASHLIGHT"/>

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon_temp_4"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".store.AlcoholReaderActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".store.InsertPriceActivity" />
        <activity android:name=".store.CheckAlcoholsActivity" />
        <activity android:name=".user.StoreReaderActivity" />
        <activity android:name=".user.AlcoholListActivity"></activity>
    </application>

</manifest>